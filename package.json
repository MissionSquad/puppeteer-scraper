  "author": {
    "name": "jayson jacobs",
    "email": "public@missionsquad.ai"
  },
  "bugs": "https://github.com/missionsquad/puppeteer-scraper/issues",
  "homepage": "https://github.com/missionsquad/puppeteer-scraper",
  "dependencies": {
    "@missionsquad/common": "^1.0.2",
    "browsers": "^1.0.2",
    "dotenv": "^16.4.7",
    "puppeteer": "^24.4.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "superlru": "^1.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-flow": "^7.24.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  }
}
